{
  "name": "acorn-theme",
  "version": "1.1.0",
  "description": "A new WordPress standalone starter theme.",
  "author": "jomurgel",
  "license": "GPL-3.0",
  "homepage": "https://github.com/jomurgel/project-acorn-theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jomurgel/project-acorn-theme"
  },
  "main": "index.js",
  "keywords": [],
  "scripts": {
    "build": "webpack --mode=production && npm run wp-pot",
    "watch": "webpack --watch",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet && stylelint \"src/**/*.{css,scss}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix && stylelint \"src/**/*.{css,scss}\" --fix",
    "wp-pot": "wp-pot --src '*.php' --dest-file 'languages/acorn-theme.pot' --domain 'acorn-theme'"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^2.1.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^5.12.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "img-loader": "^3.0.1",
    "install": "^0.12.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.1",
    "npm": "^6.14.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-unit-conversion": "0.0.3",
    "sass-loader": "^7.1.0",
    "sasslint-webpack-plugin": "^1.0.4",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-wordpress": "^13.1.0",
    "stylelint-order": "^2.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-sprite-loader": "^4.1.3",
    "svg-spritemap-webpack-plugin": "^3.2.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.2.1",
    "webpack-fix-style-only-entries": "^0.1.0",
    "wp-pot-cli": "^1.2.2"
  }
}
